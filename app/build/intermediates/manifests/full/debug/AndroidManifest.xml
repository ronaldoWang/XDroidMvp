<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.droidlover.xdroidmvp.demo"
    android:versionCode="1"
    android:versionName="1.0.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name="cn.droidlover.xdroidmvp.demo.App"
        android:allowBackup="true"
        android:icon="@mipmap/xdroid_logo_128"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="cn.droidlover.xdroidmvp.demo.ui.SplashActivity"
            android:icon="@mipmap/xdroid_logo_128" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="cn.droidlover.xdroidmvp.demo.ui.MainActivity" />
        <activity android:name="cn.droidlover.xdroidmvp.demo.ui.AboutActivity" />
        <activity android:name="cn.droidlover.xdroidmvp.demo.ui.WebActivity" />
        <activity android:name="cn.droidlover.xdroidmvp.demo.ui.LoginActivity" />
    </application>

</manifest>